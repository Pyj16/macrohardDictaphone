{
  "expo": {
    "name": "macrohardDictaphone",
    "slug": "macrohardDictaphone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "macroharddictaphone",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "googleClientId": "461690677221-m30b4qeutola3j0od96acbd2n3o2u50s.apps.googleusercontent.com",
      "googleAndroidClientId": "461690677221-4dhmdeimr572las0kuhof41tsv7ddodb.apps.googleusercontent.com",
      "router": {},
      "eas": {
        "projectId": "ba8e2126-360d-4d58-8834-db56454636ce"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.peklar.macrohardDictaphone"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.461690677221-iiubqglurpquo77jp35cir0q47lcat4p"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
